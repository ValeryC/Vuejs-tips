2C,EAAnC,UAAzB,KAAyB,EAAlB,KAAkB,EAAb;yBAAxB,YAAA,CAAwE,IAAxE,EAAwE;AAAhC,MAAA,GAAG,EAAE;AAA2B,KAAxE,EAAkD,gBAAA,CAAI,KAAK,CAAC,IAAV,CAAlD,EAAgE;AAAA;AAAhE,K;GAAwE,CAAxE,E;;AAAA,GACG,EAHL,CAQM,EAJN,YAAA,CAGK,IAHL,EAGK,IAHL,EAGK,CAFH,WAEG,G,kBADH,YAAA,CAAyE,SAAzE,EAAyE,IAAzE,EAAyE,WAAA,CAA5C,MAAA,CAAA,QAA4C,EAApC,UAAzB,KAAyB,EAAlB,KAAkB,EAAb;yBAAxB,YAAA,CAAyE,IAAzE,EAAyE;AAAjC,MAAA,GAAG,EAAE;AAA4B,KAAzE,EAAkD,gBAAA,CAAI,KAAK,CAAC,KAAV,CAAlD,EAAiE;AAAA;AAAjE,K;GAAyE,CAAzE,E;;AAAA,GACG,EAHL,CAIM,CATJ,CAUE,CAvCN,C","sourcesContent":["<template>\n    <div>\n        <h1> <router-link to=\"/v-bind\">v-bind</router-link> with hover</h1>\n         <span v-bind:title=\"message\">\n             Passez votre souris sur moi pendant quelques secondes\n             pour voir mon titre lié dynamiquement !\n        </span>\n        <h1>v-if Seen if data is true, v-else data is false</h1>\n        \n        <div v-if=\"seen\">\n          <p>This is a v-if. You see me cause seen===true</p>\n          <button @click=\"changeValue\">Change to false </button>\n        </div>\n\n        <div v-else>\n          <p>This is a v-else. You see me cause seen===false</p>\n          <button @click=\"changeValue\">Change to true</button>\n        </div>\n        <h1>Display in todos with a v-for</h1>\n         <ol>\n            <li v-for=\"todo in todos\" :key=\"todo\">{{ todo.text }}</li>\n        </ol>\n         <h1>Reverse message</h1>\n          <p>{{ msg }}</p>\n         <button v-on:click=\"reverseMessage\">Reverse message</button>\n        <input v-model=\"msg\">\n\n        {{msg.split('').reverse().join('')}}\n        \n          <h1>Parent to child with props</h1>\n        <div class=\"container\">\n      <ul>\n        <h2>the item</h2>\n        <li v-for=\"(value, index) in listdata\" :key=\"index\">{{value.item}} </li>\n      </ul>\n      <ul>\n        <h2>the color</h2>\n        <li v-for=\"(value, index) in listdata\" :key=\"index\">{{value.color}} </li>\n      </ul> \n      </div>\n    </div>\n</template>\n\n<script>\nexport default {\n   name: 'Main',\n   props: {\n    'listdata': Array,\n  },\n    data() {\n        return { \n          message: 'Vous avez affiché cette page le ' + new Date().toLocaleString(),\n          seen: true, \n          todos: [\n            { text: 'Apprendre JavaScript' },\n            { text: 'Apprendre Vue' },\n            { text: 'Créer quelque chose de génial' }\n          ],\n          msg: 'Hello Vue.js !'\n        }\n    },\n    methods: {\n        changeValue(){\n            console.log(this.seen)\n            if(this.seen ==true){\n                return this.seen = false\n                }\n            else{\n                return this.seen = true\n            }    \n        },\n        reverseMessage: function () {\n            this.msg = this.msg.split('').reverse().join('')\n        }\n    },\n}\n</script>\n\n<style scoped>\n.container{\n  position: relative;\n  display: flex;\n  justify-content: center;\n  margin-top: -40px;\n}\n\nul{\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  list-style: none;\n  text-align: left;\n}\n\nli{\n  list-style: none;\n  text-align: center;\n}\n</style>\n"],"sourceRoot":""}]}